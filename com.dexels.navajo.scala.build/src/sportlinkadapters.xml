<?xml version="1.0" encoding="UTF-8"?>
<adapterdef>
	<!-- Club SQL adapter -->
	<map>
		<tagname>clubsql</tagname>
		<object>com.dexels.sportlink.adapters.ClubDatasource</object>
		<values>
			<value name="query" type="string" required="false" direction="in" />
			<value name="update" type="string" required="false" direction="in" />
			<value name="debug" type="boolean" required="false" direction="in" />
			<value name="transactionContext" type="integer" required="false"
				direction="in" />
			<value name="datasource" type="string" required="false"
				direction="in" />
			<value name="username" type="string" required="false"
				direction="in" />
			<value name="resultSet" map="resultrow []" required="false"
				direction="out" />
		</values>
		<methods>
			<method name="addParameter">
				<param name="value" field="parameter" type="any" required="false" />
			</method>
			<method name="doUpdate">
				<param name="value" field="doUpdate" type="boolean" required="automatic"
					value="true" />
			</method>
			<method name="rollback">
				<param name="value" field="kill" type="boolean" required="automatic"
					value="true" />
			</method>
		</methods>
	</map>
	<!-- Code dropdown list -->
	<map>
		<tagname>codedropdown</tagname>
		<object>com.dexels.sportlink.adapters.CodeDropdownList</object>
		<values>
			<value name="datasource" type="string" required="false"
				direction="in" />
			<value name="transactionContext" type="integer" required="false"
				direction="in" />
			<value name="language" type="string" required="true" direction="in" />
			<value name="typeOfCode" type="string" required="true"
				direction="in" />
			<value name="name" type="string" required="true" direction="in" />
			<value name="debug" type="string" required="false" direction="in" />
			<value name="description" type="string" required="false"
				direction="in" />
			<value name="orderByDescription" type="string" required="false"
				direction="in" />
			<value name="direction" type="string" required="true"
				direction="in" />
			<value name="emptyOption" type="boolean" required="false"
				direction="in" />
			<value name="selectedValue" type="any" required="false"
				direction="in" />
		</values>
	</map>
	<!-- Dropdown list -->
	<map>
		<tagname>dropdown</tagname>
		<object>com.dexels.sportlink.adapters.DropdownList</object>
		<values>
			<value name="datasource" type="string" required="false"
				direction="in" />
			<value name="transactionContext" type="integer" required="false"
				direction="in" />
			<value name="name" type="string" required="true" direction="in" />
			<value name="debug" type="string" required="false" direction="in" />
			<value name="query" type="string" required="false" direction="in" />
			<value name="description" type="string" required="false"
				direction="in" />
			<value name="orderByDescription" type="string" required="false"
				direction="in" />
			<value name="direction" type="string" required="true"
				direction="in" />
			<value name="emptyOption" type="boolean" required="false"
				direction="in" />
			<value name="selectedValue" type="string" required="false"
				direction="in" />
			<value name="removeExisting" type="boolean" required="false"
				direction="in" />
		</values>
		<methods>
			<method name="addParameter">
				<param name="value" field="parameter" type="any" required="false" />
			</method>
		</methods>
	</map>
	<!-- GenericFieldMap adapter -->
	<map>
		<tagname>genericproperty</tagname>
		<object>com.dexels.sportlink.adapters.GenericFieldMap</object>
		<values>
			<value name="datasource" type="string" required="false"
				direction="in" />
			<value name="transactionContext" type="integer" required="false"
				direction="in" />
			<value name="query" type="string" required="false" direction="in" />
			<value name="update" type="string" required="false" direction="in" />
			<value name="singleValue" type="object" required="false"
				direction="in" />
		</values>
		<methods>
			<method name="addParameter">
				<param name="value" field="parameter" type="any" required="true" />
			</method>
			<method name="setPropertyPrefix">
				<param name="value" field="propertyPrefix" type="string"
					required="true" />
			</method>
			<method name="setTablePrefix">
				<param name="value" field="tablePrefix" type="string"
					required="true" />
			</method>
			<method name="setTableElementPrefix">
				<param name="value" field="tableElementPrefix" type="string"
					required="true" />
			</method>
			<method name="setPrimaryKey">
				<param name="value" field="primaryKey" type="string" required="true" />
			</method>
			<method name="setValueColumn">
				<param name="value" field="valueColumn" type="string"
					required="true" />
			</method>
			<method name="setTable">
				<param name="value" field="table" type="string" required="true" />
			</method>
			<method name="setCodeTable">
				<param name="value" field="codeTable" type="string" required="true" />
			</method>
			<method name="setCurrentInputMessage">
				<param name="value" field="currentInputMessage" type="string"
					required="true" />
			</method>
			<method name="addNamedParameter">
				<param name="name" field="parameterName" type="string"
					required="true" />
				<param name="value" field="parameterValue" type="any"
					required="true" />
			</method>
			<method name="doUpdate">
				<param name="value" field="doUpdate" type="boolean" required="automatic"
					value="true" />
			</method>
			<method name="doQuery">
				<param name="value" field="doQuery" type="boolean" required="automatic"
					value="true" />
			</method>
		</methods>
	</map>





	<map>
