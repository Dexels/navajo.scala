			<value name="valid" type="string" required="false" direction="out" />
			<value name="firstTimeUse" type="boolean" required="false"
				direction="out" />
			<value name="expiryTime" type="integer" required="false"
				direction="in" />
			<value name="username" type="string" required="false"
				direction="in" />
			<value name="password" type="string" required="false"
				direction="in" />
			<value name="region" type="string" required="false" direction="in" />
			<value name="token" map="string" required="false" direction="out" />
		</values>
		<methods>

